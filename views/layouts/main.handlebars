<!DOCTYPE html>
<html>

<head>
    <title>Sample Page</title>
</head>

<body>
    <nav>
        <ul>
            <li><a href="/">Home</a></li>
            <li><a href="/dashboard">Dashboard</a></li>
            {{#if loggedIn}}
            <li><a href="/logout">Logout</a></li>
            {{else}}
            <li><a href="/login">Login</a></li>
            {{/if}}
        </ul>
    </nav>

    <section>
        <!-- This is where you can add your cards later -->
    </section>

    <!-- Include Handlebars library -->
    <script src="handlebars.js"></script>

    <!-- Your Handlebars template goes here -->
    <script id="card-template" type="text/x-handlebars-template">
        <!-- Example card template -->
    <div class="card">
        <h2>{{cardTitle}}</h2>
        <p>{{cardContent}}</p>
    </div>
    </script>

    <!-- Your JavaScript to render the cards using Handlebars goes here -->
    <script>
        // Sample data for a card (replace with your data)
        var cardData = {
            cardTitle: "Card Title",
            cardContent: "This is the card content."
        };

        // Get the Handlebars template
        var source = document.getElementById("card-template").innerHTML;

        // Compile the Handlebars template
        var template = Handlebars.compile(source);

        // Render the card with data into the section
        document.querySelector("section").innerHTML = template(cardData);
    </script>
</body>

</html>